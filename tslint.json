{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "trailing-comma": false,
      "no-console": false,
      "no-bitwise": false,
      "object-literal-key-quotes": [true, "as-needed"],
      "array-type": [true, "array"],
      "prefer-for-of": true,
      "prefer-const": true
  },
  "rulesDirectory": []
}